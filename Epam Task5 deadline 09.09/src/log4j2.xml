<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<File name = "logFile" fileName = "logs/parseLogs.log" append="true">
			<PatternLayout pattern = "%d %p %c{2} [%t] - %m%n"/>
		</File>
	</Appenders>
	
	<Loggers>
		<Root level = "debug">
			<AppenderRef ref = "logFile"/>
		</Root>
	</Loggers>
</Configuration>